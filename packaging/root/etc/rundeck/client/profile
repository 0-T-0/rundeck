RDECK_HOME=/etc/rundeck/client
export RDECK_HOME

RDECK_BASE=/etc/rundeck/client
export RDECK_BASE

JAVA_HOME=/usr/lib/jvm/java/
export JAVA_HOME

PATH=$PATH:$JAVA_HOME/bin
export PATH

if test -t 0 -a -z "$RUNDECK_CLI_TERSE"
then
  RUNDECK_CLI_TERSE=true
  export RUNDECK_CLI_TERSE
fi

if test -n "$JRE_HOME"
then
   unset JRE_HOME
fi

umask 002
